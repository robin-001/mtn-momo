{
    "name": "angstrom/mtn-momo",
    "description": "A Laravel package for MTN Mobile Money API integration (Collections, Disbursements, and Remittances)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "mtn",
        "momo",
        "mobile money",
        "payment",
        "collections",
        "disbursements",
        "remittances"
    ],
    "authors": [
        {
            "name": "Angstrom",
            "email": "info@angstrom.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/support": "^10.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Angstrom\\MoMo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Angstrom\\MoMo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Angstrom\\MoMo\\MoMoServiceProvider"
            ],
            "aliases": {
                "MoMo": "Angstrom\\MoMo\\Facades\\MoMo"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
